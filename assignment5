#Assignment 5
#Task 1

student_marks = {"Alice": 85, "Bob": 78, "Charlie": 92, "Diana": 88}

name = input("Enter the student's name: ")


if name in student_marks:
    print(f"{name}'s marks: {student_marks[name]}")
else:
    print("Student not found in the records.")


#Task 2

numbers = list(range(1, 11))

first_five = numbers[0:5]

reversed_first_five = first_five.copy()
reversed_first_five.reverse()


print("Original list:", numbers)
print("First five elements:", first_five)
print("Reversed first five elements:", reversed_first_five)
